{
  "expo": {
    "name": "cctv-monitoring-app-mobile",
    "slug": "cctv-monitoring-app-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0d939d52-2ee1-4cf7-990c-3db99e7e8944"
      }
    },
    "android": {
      "package": "com.balalii_26.boltexponativewind"
    }
  }
}
